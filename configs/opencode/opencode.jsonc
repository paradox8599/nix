{
	"$schema": "https://opencode.ai/config.json",
	// Theme configuration
	"theme": "opencode",
	"permission": {
		"edit": "ask",
		"bash": "ask",
		"webfetch": "allow"
	},
	"provider": {
		"liftai": {
			"npm": "@ai-sdk/openai-compatible",
			"id": "liftai",
			"name": "LiftAI",
			"options": {
				"baseURL": "https://liftai.icu/v1",
				"apiKey": "{env:LIFTAI_API_KEY}"
			},
			"models": {
				"gemini-2.5-pro": {
					"id": "gemini-2.5-pro",
					"name": "Gemini 2.5 Pro",
					"limit": {
						"context": 1000000,
						"output": 1000000
					}
				},
				"gemini-2.5-flash": {
					"id": "gemini-2.5-flash",
					"name": "Gemini 2.5 Flash",
					"limit": {
						"context": 1000000,
						"output": 1000000
					}
				},
				"gemini-2.5-flash-lite": {
					"id": "gemini-2.5-flash-lite",
					"name": "Gemini 2.5 Flash Lite",
					"limit": {
						"context": 1000000,
						"output": 1000000
					}
				},
				"DeepSeek-R1-0528": {
					"id": "DeepSeek-R1-0528",
					"name": "DeepSeek R1 0528",
					"limit": {
						"context": 65000,
						"output": 65000
					}
				},
				"qwen3-coder": {
					"id": "qwen3-coder",
					"name": "Qwen3 Coder",
					"limit": {
						"context": 262144,
						"output": 262144
					}
				}
			}
		}
	},
	"disabled_providers": ["openai"],
	"model": "liftai/gemini-2.5-pro",
	"small_model": "liftai/gemini-2.5-flash",
	"autoupdate": true
}
